<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<!--Checking pushes work-->

<AnchorPane fx:id="rootPane" prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.swiss_army_app.dashboard.DashboardController">
   <Label fx:id="welcomeLabel" layoutX="205.0" layoutY="91.0" prefHeight="52.0" prefWidth="658.0" textAlignment="CENTER" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="36.0" />
      </font>
   </Label>
   <Button id="toTipCalc" layoutX="206.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#loadTipCalc" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #c42121; -fx-background-radius: 10;" text="Tip Calculator" textAlignment="CENTER" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="24.0" />
      </font></Button>
   <Button id="toNumberGuess" layoutX="433.0" layoutY="186.0" mnemonicParsing="false" onMouseClicked="#loadNumGuess" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #c42121; -fx-background-radius: 10;" text="Guess the Number" textAlignment="CENTER" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="18.0" />
      </font></Button>
   <Button id="toCatBox" layoutX="659.0" layoutY="185.0" mnemonicParsing="false" onMouseClicked="#loadCat" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #c42121; -fx-background-radius: 10;" text="Cat in a Box" textAlignment="CENTER" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="24.0" />
      </font></Button>
   <Button id="toVerse" layoutX="209.0" layoutY="323.0" mnemonicParsing="false" onMouseClicked="#loadVerse" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #c42121; -fx-background-radius: 10;" text="Verse of the Day" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="21.0" />
      </font></Button>
   <Button id="toSports" layoutX="435.0" layoutY="322.0" mnemonicParsing="false" onMouseClicked="#loadSports" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #c42121; -fx-background-radius: 10;" text="Sports Stats" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="25.0" />
      </font></Button>
   <HBox prefHeight="600.0" prefWidth="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
          <fx:include source="../sidebar/sidebar.fxml" />
      </children>
   </HBox>
</AnchorPane>
